package com.OOPS;

class A
{
	public void printA() {
	System.out.println("It is class A");
	
}
class B extends A
{
	public void printB()
	{
	System.out.println("It is class B");
	}
}

public class Inheritance {
		
	public static void main(String args[])
	{
		A a=new A();
		a.printA();
		
		B b=new B();
		b.printB();
		
		b.printA();
	}
		
	}

}
